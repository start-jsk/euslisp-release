(in-package "GL")

(setq *objdir*
  (format nil "~A/~A/obj/" *eusdir* (unix:getenv "ARCHDIR"))
  )

(comp:compile-file-if-src-newer "irtgl.l" *objdir*)
(comp:compile-file-if-src-newer "irtglrgb.l" *objdir*)
(comp:compile-file-if-src-newer "irtviewer.l" *objdir*)

(exit 0)

